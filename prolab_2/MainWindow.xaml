<Window x:Class="prolab_2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prolab_2"
        mc:Ignorable="d"
        Title="isistasyonu" 
        Height="600" 
        Width="1000"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen" 
        Background="Black">
    <Canvas x:Name="GameCanvas" Background="DarkGray" Margin="10">
        <Button x:Name="btnEnemy2" 
                Content="Enemy2" 
                Canvas.Left="470" 
                Canvas.Top="10" 
                Height="50" Width="60" 
                HorizontalAlignment="Center" VerticalAlignment="Top"
                Click="btnEnemy2_Click"
                Visibility="Hidden"/>
        <Button x:Name="btnEnemy" 
                Content="Enemy" 
                Canvas.Left="920" Canvas.Top="180" 
                Height="50" Width="60" 
                Click="btnEnemy_Click"
                HorizontalAlignment="Center" VerticalAlignment="Top"/>
    </Canvas>
</Window>
